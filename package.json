{
  "name": "y",
  "version": "1.0.0",
  "description": "Implements a Books API for a Library, intended to be run on AWS using serverless",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "dependencies": {
    "serverless-pseudo-parameters": "^2.5.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chance": "^1.1.6",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "ghooks": "^2.0.4",
    "mocha": "^7.2.0",
    "request": "^2.88.2",
    "sinon": "^9.0.2"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "test": "mocha test/**/*.test.js"
  },
  "config": {
    "ghooks": {
      "pre-commit": "opt --in pre-commit --exec 'npm run lint'",
      "pre-push": "opt --in pre-push --exec 'npm run test'"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjgs/serverless-books-api.git"
  },
  "keywords": [
    "serverless",
    "cloud",
    "aws",
    "api",
    "rest"
  ],
  "author": "Mark Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mjgs/serverless-books-api/issues"
  },
  "homepage": "https://github.com/mjgs/serverless-books-api#readme"
}
